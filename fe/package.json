{
  "name": "lote",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "BUILD_MODE=build vue-tsc --noEmit && vite build && vite --config note.vite.config.ts build --emptyOutDir=false",
    "build-notes": "BUILD_MODE=build-notes vite --config note.vite.config.ts build",
    "build-single-note": "BUILD_MODE=build-single-note vite --config note.vite.config.ts build",
    "deploy": "npm run build && node ./bin/deploy",
    "deploy-notes": "npm run build-notes && node ./bin/deploy-notes",
    "deploy-single-note": "npm run build-single-note && node ./bin/deploy-note",
    "delete-note": "node ./bin/delete-note",
    "serve": "vite preview"
  },
  "dependencies": {
    "axios": "^0.23.0",
    "postcss-import": "^14.0.2",
    "vue": "^3.2.6",
    "vue-i18n": "^9.1.10",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.3.4",
    "@types/node": "^16.11.4",
    "@vitejs/plugin-vue": "^1.6.1",
    "@vue/compiler-sfc": "^3.2.6",
    "autoprefixer": "^10.3.7",
    "fs-extra": "^10.1.0",
    "postcss": "^8.3.11",
    "tailwindcss": "^2.2.17",
    "typescript": "^4.3.2",
    "vite": "^2.5.4",
    "vue-tsc": "^0.2.2"
  }
}
